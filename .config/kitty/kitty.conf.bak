include ~/.cache/wal/colors-kitty.conf
#Font
# font_family JetBrainsMonoNL Nerd Font
# italic_font auto
# bold_italic_font auto
font_size 14.0

# Theme
# foreground #fafafa
# background #1f1f1f

# Cursor
shell_integration no-cursor
# cursor #cc9900
# cursor #fafafa
cursor_shape block
# cursor_beam_thickness 2.5

disable_ligatures cursor

# window_padding_width 2
background_opacity 0.8

url_color #0087bd
url_style curly

# BEGIN_KITTY_FONTS
font_family      family="JetBrainsMonoNL Nerd Font"
bold_font        auto
italic_font      auto
bold_italic_font auto
# END_KITTY_FONTS
