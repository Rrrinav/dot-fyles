include ~/.cache/wal/colors-kitty.conf


# Theme
# foreground #fafafa
# background #1f1f1f
font_size 14.0
# Cursor
shell_integration no-cursor
# cursor #cc9900
# cursor #fafafa
cursor_shape block
# cursor_beam_thickness 2.5

# disable_ligatures cursor
bell off

# window_padding_width 2
background_opacity 0.8

url_color #0087bd
url_style curly

# BEGIN_KITTY_FONTS
font_family      family='Lilex Nerd Font Mono' style=ExtraLight
bold_font        family='Lilex Nerd Font Mono' style=Regular
italic_font      family='Lilex Nerd Font Mono' style=ExtraLight features='+ss03 +zero +ss02'
bold_italic_font family='Lilex Nerd Font Mono' style=Regular
# END_KITTY_FONTS
